@model DSPA_MeAudVis.Web.Models.AdministratorViewModel

@{
    ViewData["Title"] = "Asign new Administrator";
}

<h2>Asign new Administrator</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <div class="form-group">

                    <div class="form-group">
                        <label asp-for="UserUserName" class="control-label"></label>
                        <select asp-for="UserUserName" asp-items="Model.Users" class="form-control"> </select>
                        <span asp-validation-for="UserUserName" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <a asp-action="Index" class="btn btn-primary"> <span class="glyphicon glyphicon-arrow-left"></span></a>
                <button type="submit" class="btn btn-success"> <span class="glyphicon glyphicon-plus"></span> </button>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

